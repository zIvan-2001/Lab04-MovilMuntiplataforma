<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Laboratorio04.MainPage">

    <StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
        <Button x:Name="Item1" Text="Navigation" Background="#ef476f" 
                CornerRadius="30" TextColor="#ffff"></Button>
        <Button x:Name="Item2" Text="TabbedPage" Background="#f77f00"
                CornerRadius="30" TextColor="#ffff"></Button>
        <Button x:Name="Item3" Text="CarouselPage" Background="#06d6a0"
                CornerRadius="30" TextColor="#ffff"></Button>
        <Button x:Name="Item4" Text="ModalPages" Background="#118ab2"
                CornerRadius="30" TextColor="#ffff"></Button>
        <Button x:Name="Item5" Text="DisplayAlert" Background="#073b4c"
                CornerRadius="30" TextColor="#ffff"></Button>
    </StackLayout>

</ContentPage>
